.PHONY: clean All

All:
	@echo "----------Building project:[ sec7_challenge - Debug ]----------"
	@cd "sec7_challenge" && "$(MAKE)" -f  "sec7_challenge.mk"
clean:
	@echo "----------Cleaning project:[ sec7_challenge - Debug ]----------"
	@cd "sec7_challenge" && "$(MAKE)" -f  "sec7_challenge.mk" clean
